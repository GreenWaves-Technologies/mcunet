set log_level INFO
set debug true
adjust
fusions --scale8
qtune --step * use_ne16=true hwc=true
set input_norm_func 'x:(x/255 - 0.5)/0.5'

# RAM/FLASH Settings
set l3_ram_device $(MODEL_L3_RAM)
set l3_flash_device $(MODEL_L3_FLASH)

set privileged_l3_flash_device $(MODEL_SEC_L3_FLASH)
set privileged_l3_flash_size 1900000

set graph_const_exec_from_flash true

nodeoption input_1 ALLOCATE 1 
set graph_produce_node_names true
set graph_produce_operinfos true
set graph_monitor_cycles true
#set graph_trace_exec true
save_state
